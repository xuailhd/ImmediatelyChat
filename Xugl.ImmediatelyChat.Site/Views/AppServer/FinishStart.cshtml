@{
    ViewBag.Title = "FinishStart";
}
@model Xugl.ImmediatelyChat.Site.Models.ServersModel
<h2>MMSs</h2>
<table>
    <tr><th>ip</th><th>port</th><th>Arrange Char</th></tr>
@foreach (var m in Model.MMSServers)
{
    <tr>
        <td>@m.MMS_IP</td>
        <td>@m.MMS_Port</td>
        <td>@m.ArrangeStr</td>
    </tr>
}
</table>
<br />
<h2>MCSs</h2>
